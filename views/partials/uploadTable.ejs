<div class="container py-5">
  <div class="table-responsive">
    <table id="data-table" class="table table-striped table-bordered">
      <thead>
        <tr>
          <th rowspan="2">身份证号</th>
          <th rowspan="2">姓名</th>
          <th rowspan="2">性别</th>
          <th colspan="9">左脚</th>
          <th colspan="9">右脚</th>
        </tr>
        <tr>
          <th>鞋码</th>
          <th>足弓长度</th>
          <th>足弓宽度</th>
          <th>后跟宽度</th>
          <th>足长</th>
          <th>足宽</th>
          <th>球围</th>
          <th>足弓指数</th>
          <th>足弓比例</th>
          <th>鞋码</th>
          <th>足弓长度</th>
          <th>足弓宽度</th>
          <th>后跟宽度</th>
          <th>足长</th>
          <th>足宽</th>
          <th>球围</th>
          <th>足弓指数</th>
          <th>足弓比例</th>
        </tr>
      </thead>
      <tbody>
        <% footDataArray.forEach(footData => { %>
        <tr>
          <td><%= footData.id_num %></td>
          <td><%= footData.name %></td>
          <td><%= footData.gender %></td>
          <td><%= footData.shoe_size_left %></td>
          <td><%= footData.arch_length_left %></td>
          <td><%= footData.arch_width_left %></td>
          <td><%= footData.heel_width_left %></td>
          <td><%= footData.foot_length_left %></td>
          <td><%= footData.foot_width_left %></td>
          <td><%= footData.ball_girth_left %></td>
          <td><%= footData.arch_index_left %></td>
          <td><%= footData.arch_ratio_left %></td>
          <td><%= footData.shoe_size_right %></td>
          <td><%= footData.arch_length_right %></td>
          <td><%= footData.arch_width_right %></td>
          <td><%= footData.heel_width_right %></td>
          <td><%= footData.foot_length_right %></td>
          <td><%= footData.foot_width_right %></td>
          <td><%= footData.ball_girth_right %></td>
          <td><%= footData.arch_index_right %></td>
          <td><%= footData.arch_ratio_right %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</div>
